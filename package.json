		"babel-preset-react": "^6.24.1",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "4.6.0",
		"cross-env": "^5.2.0",
		"css-loader": "0.28.11",
		"express": "^4.17.1",
		"extract-text-webpack-plugin": "3.0.2",
		"file-loader": "^1.1.11",

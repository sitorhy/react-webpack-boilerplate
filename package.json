		"build": "cross-env NODE_ENV=production webpack --progress",
		"watch": "cross-env NODE_ENV=production webpack --progress --watch"
	},
	"keywords": [
		"webpack-ie8",
		"ie8",
		"react",
		"react-ie8",
		"redux-ie8",
		"react-router-ie8",
		"anujs"
	],
